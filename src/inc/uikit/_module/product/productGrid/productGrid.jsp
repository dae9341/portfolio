<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<div class="m-productGrid">
    <%--<a href="#" class="m-productGrid__link"></a>--%>
    <div class="m-productGrid__item">
        <!--상품 이미지 영역-->
        <div class="m-productGrid__item__thumb">
            <img class="m-productGrid__item__thumb__image" alt="" src="">
            <div class="m-productGrid__item__thumb__labeImages">
                <div class="a-imageLabel">
                    <img class="" src="" alt="">
                </div>
                <div class="a-imageLabel">
                    <img class="" src="" alt="">
                </div>
                <div class="a-imageLabel">
                    <img class="" src="" alt="">
                </div>
            </div>
        </div>
        <!--//상품 이미지 영역-->

        <!--상품 정보 영역-->
        <div class="m-productGrid__item__content">

            <!--상품명-->
            <div class="m-productGrid__item__nameInfo">
                <div class="m-productGrid__item__nameInfo__brand">LG전자</div>
                <div class="m-productGrid__item__nameInfo__name">[쿠폰할인 50%] LG 올레드 OLED TV OLED55A1ENA 55인치</div>
            </div>
            <!--//상품명-->

            <!--가격-->
            <div class="m-productGrid__item__priceInfo">
                <span class="m-productGrid__item__priceInfo__percent">69%</span>
                <span class="m-productGrid__item__priceInfo__price">99,000</span>
            </div>
            <!--//가격-->

            <!--상품평-->
            <div class="m-productGrid__item__reviewInfo">
                <div class="m-productGrid__item__reviewInfo__score">4.5</div>
                <div class="m-productGrid__item__reviewInfo__review">상품평 <span>2건</span></div>
            </div>
            <!--//상품평-->

            <!--라벨-->
            <div class="m-productGrid__item__labelInfo">
                <span class="a-textLabel">무료배송</span>
                <span class="a-textLabel -special">특가</span>
            </div>
            <!--//라벨-->

            <!--즐겨찾기-->
            <div class="m-productGrid__item__bookmark">
                <a href="#" data-bookmark="false">북마크버튼임</a>
            </div>
            <!--//즐겨찾기-->

        </div>
        <!--//상품 정보 영역-->
    </div>
</div>